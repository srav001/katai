{
	"name": "katai",
	"version": "0.1.2",
	"scripts": {
		"dev": "vite dev",
		"build": "npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package -o dist && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./primitives": {
			"types": "./dist/store/primitives.svelte.d.ts",
			"import": "./dist/store/primitives.svelte.js"
		},
		"./cache-adapters": {
			"types": "./dist/cache-adapters/index.d.ts",
			"import": "./dist/cache-adapters/index.js"
		},
		"./stores": {
			"types": "./dist/stores/index.d.ts",
			"import": "./dist/stores/index.js"
		}
	},
	"files": [
		"dist"
	],
	"peerDependencies": {
		"svelte": ">=5.0.0",
		"idb-keyval": "^6.2.1"
	},
	"devDependencies": {
		"eslint-compat-utils": "^0.1.2",
		"eslint-scope": "^7.2.2",
		"eslint-visitor-keys": "^3.4.3",
		"svelte-eslint-parser": "^0.33.1",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/package": "^2.2.6",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/node": "^20.11.7",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-sonarjs": "^0.23.0",
		"eslint-plugin-svelte": "^2.30.0",
		"idb-keyval": "^6.2.1",
		"prettier": "^3.1.1",
		"prettier-plugin-organize-imports": "^3.2.4",
		"prettier-plugin-svelte": "^3.1.2",
		"publint": "^0.2.7",
		"svelte": "^5.0.0-next.70",
		"svelte-check": "^3.6.0",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.1.4",
		"vitest": "^1.0.0"
	},
	"description": "Katai (meaning store in Tamil) is a simple state manager for Svelte 5",
	"dependencies": {},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/srav001/katai.git"
	},
	"keywords": [
		"store",
		"state",
		"svelte",
		"svelte5",
		"svelte-5",
		"svelte 5",
		"svelteKit",
		"svelte-kit",
		"svelte kit"
	],
	"author": "Sravan Suresh",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/srav001/katai/issues"
	},
	"homepage": "https://github.com/srav001/katai#readme"
}
